<script>
function setBoardIframe(){
	var docId = getDocId("1100001014");
	var url;
	if(docId){
		url = "//ui.nboard2.naver.com/nboard2/read/1100001014/" + docId + "?n2_cntPerPage=15";
	}else{
		url = "//ui.nboard2.naver.com/nboard2/list/1100001014?n2_cntPerPage=15";
	}
	document.getElementById("NAVER_COMMON_BOARD_IFRAME").src = url;
}
function getDocId(boardId){
	return String(location).split(boardId+"/")[1];
}
setBoardIframe();
</script>
